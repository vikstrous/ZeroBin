{
	"manifest_version": 2,
	"name": "ZeroBin",
	"description": "Upload encrypted files and messages anywhere.",
	"version": "0.0.1",
	"app": {
		"launch": {
			"local_path": "page.html"
		}
	},
	"permissions": [
		"http://zerobin.local/"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
